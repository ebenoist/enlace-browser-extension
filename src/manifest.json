{
  "name": "Enlace Exporter",
  "description": "Browser extension to save selected pages or browsing history to enlace.space",
  "version": "0.0.1",
  "manifest_version": 2,

  "icons": {
    "128": "static/128.png"
  },

  "browser_action": {
    "default_title": "Enlace Exporter",
    "default_popup": "static/action.html"
  },

  "background": {
    "scripts": [ "background.js" ]
  }
}
